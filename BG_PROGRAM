IF (UI[5:Fault reset]=ON) THEN ;
DO[7]=OFF ;
ENDIF ;
IF (DI[9]=ON) THEN ;
DO[15]=OFF ;
ENDIF ;
IF (UI[5:Fault reset]=ON) THEN ;
DO[22]=OFF ;
ENDIF ;
DO[8]=($MOR_GRP[1].$ROB_MOVE) ;
IF (DI[15]=ON OR DI[16]=ON OR DI[17]=ON OR DI[18]=ON OR DI[23]=ON OR DI[24]=ON OR DI[25]=ON OR DI[26]=ON) THEN ;
DO[17]=ON ;
DO[18]=OFF ;
ENDIF ;
IF (DI[19]=ON OR DI[20]=ON OR DI[21]=ON OR DI[22]=ON OR DI[27]=ON OR DI[28]=ON OR DI[29]=ON OR DI[30]=ON) THEN ;
DO[17]=OFF ;
DO[18]=ON ;
ENDIF ;
POS
END
