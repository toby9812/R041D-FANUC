R[135]=(((PR[89,2]-PR[80,2])/7)*R[131]) ;
R[136]=R[15]*(-1) ;
PR[83]=PR[80] ;
PR[83,1]=PR[80,1]+R[134] ;
PR[83,2]=PR[80,2]+R[135] ;
PR[83,3]=PR[80,3]+R[133] ;
PR[82]=PR[83] ;
