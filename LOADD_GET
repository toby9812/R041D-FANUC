R[2]=1 ;
J PR[2] R[52]% FINE ;
R[2]=2 ;
J PR[3] R[52]% FINE ;
R[2]=3 ;
ENDIF ;
IF (R[2]=4) THEN ;
L PR[4] R[51]mm/sec FINE ;
R[2]=4 ;
L PR[1] R[51]mm/sec FINE ;
R[2]=1 ;
J PR[2] R[52]% FINE ;
R[2]=2 ;
J PR[3] R[52]% FINE ;
R[2]=3 ;
ENDIF ;
--chSTAR
L PR[3] R[51]mm/sec FINE ;
R[2]=3 ;
L PR[88] R[51]mm/sec FINE ;
R[2]=88 ;
DO[3]=ON ;
